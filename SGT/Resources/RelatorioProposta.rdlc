	<Code>
		Public Function NUMBERS_FROM_STRING(ByVal strText As String) As String

		Dim myChars() As Char = strText.ToCharArray()
		Dim strTextNumbers As String = ""
		Dim lValue As Long = 0

		For Each ch As Char In myChars
		If Char.IsDigit(ch) Then
		strTextNumbers = strTextNumbers &amp; ch.ToString
		End If
		Next

		If String.IsNullOrEmpty(strTextNumbers) Then
		lValue = 0
		Else
		lValue = Long.Parse(strTextNumbers)
		End If

		Return lValue.ToString

		End Function

		Public Function REMOVE_CODES_IN_DESCRIPTION(ByVal strDescription As String) As String

		Dim arrRemove() As String = Split("(CÓD,( CÓD,(COD,( COD,(CÃ?,( CÃ?", ",")
		Dim tmpText As String = strDescription

		For x As Integer = LBound(arrRemove) To UBound(arrRemove)
		If InStr(tmpText, arrRemove(x)) &gt; 0 Then tmpText = Strings.Trim(Left(tmpText, InStr(tmpText, arrRemove(x)) - 1))
		Next

		Return tmpText

		End Function

		Public Function GET_WORD(TextWord As String) As String

		Dim vgword() As String
		Dim fWord As String
		Dim lWord As String

		vgword = Split(TextWord, " ")
		fWord = vgword(LBound(vgword))
		lWord = vgword(UBound(vgword))

		if fWord = lWord then
		return fWord
		else
		Return fWord &amp; " " &amp; lWord
		end if

		Return ""

		End Function

		Public Function Formata_Telefone(ByVal Telefone As Object) As String

		Select Case Len(Telefone.ToString)
		Case Is = 0
		Return ""
		Case Is &lt;= 10
		Return String.Format("{0:(##) ####-####}", Long.Parse(Telefone))
		Case Else
		Return String.Format("{0:(##) #####-####}", Long.Parse(Telefone))
		End Select
		Return ""
		End Function
